<script>
	var deviceKbn = (function(){
	    var ua = navigator.userAgent;
	    if(ua.indexOf('iPhone') > 0 || ua.indexOf('iPod') > 0 || ua.indexOf('Android') > 0 && ua.indexOf('Mobile') > 0){
	        return 'sp';
	    }else if(ua.indexOf('iPad') > 0 || ua.indexOf('Android') > 0){
	        return 'tab';
	    }else{
	        return 'other';
	    }
	})();
	jQuery(function($){
		$(document).ready(function(){
			$('.a.bxslider').bxSlider({
				mode: 'vertical',
				randomStart: true,
				moveSlides: 1,
				slideMargin: 10,
				infiniteLoop: true,
				//slideWidth: 100,
				minSlides: 3,
				maxSlides: 20,
				speed: 1000,
				controls: false,
				auto: true,
			});
			$('.b.bxslider').each(function(){
				var _el = $(this);
					_el.bxSlider({
					mode: 'horizontal',
					randomStart: true,
					moveSlides: 1,
					slideMargin: 10,
					infiniteLoop: true,
					//slideWidth: 100,
					minSlides: 2,
					maxSlides: 10,
					speed: 1000,
					auto: true,
					height: 300
				});
				var _height;
				switch(deviceKbn){
					case 'sp':
				    		_height = _el.data('sp-height');
						break;
					case 'tab':
				    		_height = _el.data('pc-height');
						break;			
					default:
					    _height = _el.data('pc-height');
					    break;
				}
				_el.parent().css('height', _height);
			});
		});
	});
</script>
